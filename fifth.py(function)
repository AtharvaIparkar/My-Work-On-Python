def add(a):
    return a**2
print(add(10))

def a(string):
    return len(string)
print("the length of string is : " ,a("ZEAL COLLEGE"))

def atharva(a,b):
    return a-b
print(atharva(20,10))

def sq(a):
    sq=[]
    for i in a:
        sq.append(i**2)
    return sq
a=[1,2,3]
print(sq(a))
print("squares of the list are : ",sq(a))

def a(n1,n2=70):
    print(n1)
    print(n2)
a(30)
a(40,50)

# Create a function to check if no. less than 10 or not
def a(b):
    if b<10:
        print("less than 10")
    else:
        print("more than 10")
a(9)


# Create a function to print even numbers upto n
n=int(input())
def fun(a):
    for i in range(1,n):
        if i%2==0:
            print(i)
fun(n)


#Create a function a check whether a person is elegible for voting or not
a=int(input())
def age(x):
    if x<18:
        print("Not Eligible")
    else:
        print("Eligible") 
age(a)


# Create a function to find largest among 3 numbers
n1=int(input())
n2=int(input())
n3=int(input())
def a(x,y,z):
    if n1>n2 and  n1>n3:
        print(n1,"is greater")
    elif n2>n1 and n2>n3:
        print(n2,"is greater")
    else:
        print(n3,"is greater")
a(n1,n2,n3)
